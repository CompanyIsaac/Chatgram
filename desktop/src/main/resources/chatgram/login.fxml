<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400" prefWidth="600" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chatgram.controllers.LoginController">
    <Label layoutX="179.0" layoutY="14.0" prefHeight="47.0" prefWidth="243.0" text="Chatgram Admin">
        <font>
            <Font size="32.0" />
        </font></Label>
    <GridPane alignment="CENTER" layoutX="125.0" layoutY="84.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="155.0" prefWidth="350.0">
        <columnConstraints>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="200.0" minWidth="100.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="73.0" minHeight="10.0" prefHeight="38.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="80.0" minHeight="10.0" prefHeight="37.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="56.0" minHeight="10.0" prefHeight="16.0" vgrow="SOMETIMES" />
         <RowConstraints maxHeight="67.0" minHeight="10.0" prefHeight="67.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <TextField fx:id="email" promptText="Email" />
        <PasswordField fx:id="pw" prefHeight="25.0" prefWidth="169.0" promptText="Password" GridPane.rowIndex="1" />
        <Label fx:id="errormessage" prefHeight="0.0" prefWidth="198.0" GridPane.rowIndex="2" />
        <Button fx:id="login" onAction="#login" prefHeight="25.0" prefWidth="90.0" text="Login" GridPane.rowIndex="3" />
    </GridPane>
</AnchorPane>
